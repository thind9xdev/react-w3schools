{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}